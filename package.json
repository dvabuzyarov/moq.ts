{
  "name": "",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "start": "ng serve",
    "build": "ng build moq",
    "build-docs": "ng build docs",
    "build-tests": "tsc -p ./projects/moq/tsconfig.spec.json ",
    "test": "npm run build-tests && node ./out-tsc/**/index.specs.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "commit-retry": "git cz --retry",
    "semantic-release": "semantic-release",
    "typedoc": "node ./dist/docs/esm2015/docs.js",
    "serve-docs": "rimraf docs/* && npm run build-docs && npm run typedoc && serve",
    "postpublish": ":> .released"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "0.12.3",
    "@angular-devkit/build-ng-packagr": "0.12.3",
    "@angular/cli": "7.2.3",
    "@angular/compiler": "7.2.3",
    "@angular/compiler-cli": "7.2.3",
    "@angular/core": "7.2.3",
    "@angular/language-service": "7.2.3",
    "@commitlint/cli": "7.4.0",
    "@commitlint/config-conventional": "7.3.1",
    "@types/jasmine": "3.3.8",
    "@types/node": "8.10.39",
    "cz-conventional-changelog": "2.1.0",
    "husky": "1.3.1",
    "jasmine": "3.3.1",
    "jasmine-core": "3.3.0",
    "ng-packagr": "4.7.0",
    "semantic-release": "15.13.3",
    "serve": "10.1.1",
    "ts-node": "8.0.2",
    "tsickle": "0.34.0",
    "tslib": "1.9.3",
    "tslint": "5.12.1",
    "typedoc": "0.14.2",
    "typescript": "3.2.4"
  },
  "release": {
    "pkgRoot": "dist/moq"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "commit-msg": "commitlint --edit"
    }
  }
}
