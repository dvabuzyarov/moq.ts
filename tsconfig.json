{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "declaration": false,
    "module": "es2022",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2022",
    "allowSyntheticDefaultImports": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "moq": [
        "dist/moq"
      ],
      "moq/*": [
        "dist/moq/*"
      ],
      "docs": [
        "dist/docs"
      ],
      "docs/*": [
        "dist/docs/*"
      ]
    }
  }
}
